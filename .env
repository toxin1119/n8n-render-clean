# 基本設定
N8N_HOST=my-n8n-service-xxxxx.onrender.com
N8N_PROTOCOL=https
WEBHOOK_TUNNEL_URL=https://my-n8n-service-xxxxx.onrender.com

# 運作模式
EXECUTIONS_MODE=regular

# 時區與語系（可選）
GENERIC_TIMEZONE=Asia/Taipei

# 安全設定
N8N_ENCRYPTION_KEY=some-long-secret-key-you-generate
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
